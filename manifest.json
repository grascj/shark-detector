{
  "manifest_version": 2,
  "name": "shark-detector",
  "version": "0.1",


  "permissions": [
	  "tabs",
	  "activeTab",
	  "webNavigation",
	  "<all_urls>",
	  "storage"
  ],


  "background": {
    "persistent": false,
    "scripts": ["resemble.js","imageanalyzer.js","screenshotter.js"]
  },



  "content_scripts": [
	  {
		  "matches": ["https://www.amazon.com/*"],
		  "js": ["shark-detector.js"]
	  }
  ],

  "author": "Chris Grasing & Kim Allan"
}
